{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/opensync/(.*)": "<rootDir>/../libs/opensync/src/$1",
    "@app/opensync": "<rootDir>/../libs/opensync/src"
  }
}
